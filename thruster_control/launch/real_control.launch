<launch>
    <arg name="model" default="v28"/>
    <param name="model" value="$(arg model)"/>

    <include file="$(find thruster_control)/launch/sim_control.launch">
        <arg name="model" value="$(arg model)" />
    </include>

    <node name="real_thruster_output" pkg="thruster_control" type="real_thruster_pwm_node.py" output="screen"/>
</launch>